<%= render 'common/navbar' %>
<div class="row">
  <table class="room-table">
    <% @rooms.each do |room| %>
      <tr class="row">
        <td class="col-2"><%= room.name %></td>
        <td class="col-10">
          <div class="room-chore-list container" >
            <%= render "room_chore_list_tile", room: room %>
          </div>
        </td>
      </tr>
    <% end %>
  </table>
</div>
